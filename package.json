{
  "name": "ssr-movie-app",
  "version": "1.0.0",
  "description": "",
  "main": ".babelrc.js",
  "scripts": {
    "start": "concurrently \"npm run server:dev\" \"npm run bundler:dev\"",
    "start:prod": "concurrently \"npm run server:prod\" \"npm run bundler:prod\"",
    "server:dev": "cross-env NODE_ENV=development nodemon -- server/server.js",
    "server:prod": "cross-env NODE_ENV=production nodemon -- server/server.js",
    "bundler:dev": "cross-env NODE_ENV=development nodemon scripts/build.js --watch ./scripts/build.js --ignore ./build",
    "bundler:prod": "cross-env NODE_ENV=production nodemon scripts/build.js --watch ./scripts/build.js --ignore ./build",
    "server-start": "webpack --config webpack.server.js",
    "server-build": "webpack --config webpack.build.js",
    "ssr:server:prod": "cross-env NODE_ENV=production node build-ssr/server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/register": "^7.21.0",
    "babel-loader": "^9.1.2",
    "babel-preset-react-app": "^10.0.1",
    "compression": "^1.7.4",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.4",
    "resolve": "^1.22.2",
    "rimraf": "^5.0.0",
    "webpack": "^5.79.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "mini-css-extract-plugin": "^2.7.5",
    "webpack-node-externals": "^3.0.0"
  }
}
